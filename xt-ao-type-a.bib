journal = {Zhong nan da xue xue bao. Yi xue ban = Journal of Central South University. Medical sciences},
keywords = {Adolescent,Adult,Aged,Female,Follow-Up Studies,Fracture Fixation, Internal,Fractures, Bone,Fractures, Compression,Humans,Joint Dislocations,Male,Middle Aged,Multiple Trauma,Spinal Fractures,Sternum,Thoracic Vertebrae,Young Adult,classification,complications,injuries,methods,surgery},
