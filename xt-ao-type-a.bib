journal = {Spine},
keywords = {Adult,Bone Plates,Bone Screws,Female,Follow-Up Studies,Fracture Fixation, Internal,Humans,Internal Fixators,Joint Dislocations,Lumbar Vertebrae,Male,Prospective Studies,Spinal Fractures,Spinal Fusion,Thoracic Vertebrae,Time Factors,injuries,methods,surgery},
