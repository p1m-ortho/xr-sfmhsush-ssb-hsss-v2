keywords = {Adult,Female,Follow-Up Studies,Humans,Internal Fixators,Kyphosis,Lumbar Vertebrae,Male,Movement,Postoperative Complications,Radiography,Retrospective Studies,Spinal Fractures,Spinal Fusion,Thoracic Vertebrae,diagnostic imaging,epidemiology,injuries,physiology,surgery},
