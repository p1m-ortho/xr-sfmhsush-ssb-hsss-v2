keywords = {Acrylates,Animals,Humans,Male,Platelet Aggregation,Platelet Aggregation Inhibitors,Prostaglandin Antagonists,Pyrans,Rabbits,Random Allocation,Receptors, Thromboxane A2, Prostaglandin H2,Thromboxane A2,antagonists {\&} inhibitors,drug effects,metabolism,pharmacology},
language = {eng},
