issn = {2005-4408 (Electronic)},
journal = {Clinics in orthopedic surgery},
keywords = {Adult,Bone Transplantation,Female,Fracture Fixation, Internal,Humans,Intervertebral Disc,Longitudinal Ligaments,Lumbar Vertebrae,Magnetic Resonance Imaging,Male,Prognosis,Spinal Fractures,Thoracic Vertebrae,Vertebroplasty,Young Adult,diagnostic imaging,injuries,surgery},
language = {eng},
month = {dec},
number = {4},
pages = {465--471},
pmid = {29201299},
title = {{The Relationship between Superior Disc-Endplate Complex Injury and Correction Loss in Young Adult Patients with Thoracolumbar Stable Burst Fracture.}},
