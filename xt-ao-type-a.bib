language = {eng},
month = {nov},
number = {4},
pages = {257--262},
pmid = {19813020},
title = {{Measurement of cytotoxic T lymphocyte activity of human cytomegalovirus seropositive individuals by a highly sensitive coupled luminescent method.}},
volume = {198},
