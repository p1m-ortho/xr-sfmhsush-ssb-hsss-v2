title = {{Lumbar intrathecal ligaments.}},
volume = {15},
year = {2002}
}
@article{Liu2000,
abstract = {OBJECTIVE: To provide a new method to estimate t he effectiveness of thoracolumbar vertebral finite element model. METHODS: A mechanical model of human thoracolumbar vertebrae mo tion segment was made using three-dimensional finite element method and the str ess distribution of vertically compressed thoracolumbar vertebrae was analyzed, meanwhile, 20 patients with burst fracture of thoracolumbar vertebrae were t ested by CT to calculated average CT value at ascertained different points of th oracolumbar vertebrae. The calculated results and effective stress at the same p osition were analyzed with straight line correlation. RESULTS: The stress level of different position of thoracolumba r vertebrae under vertical compressive force was positively correlated with the correlative CT value, and the regressive style, Y=214.028+45.268X, r=0.7386, P{\textless}0.05 (n=8) showed a statistical significance. CONCLUSIONS: To study mechanism of thoracolumbar vertebrae in juries under different forces has clinical significance.},
