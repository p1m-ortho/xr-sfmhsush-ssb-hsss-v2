journal = {Bulletin of the Hospital for Joint Disease (2013)},
keywords = {Fracture Fixation,Fracture Healing,Humans,Lumbar Vertebrae,Magnetic Resonance Imaging,Minimally Invasive Surgical Procedures,Postoperative Complications,Risk Factors,Spinal Fractures,Thoracic Vertebrae,Treatment Outcome,adverse effects,diagnostic imaging,etiology,methods,physiopathology,surgery},
language = {eng},
month = {mar},
