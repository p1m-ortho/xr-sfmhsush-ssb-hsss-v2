issn = {1432-0932 (Electronic)},
journal = {European spine journal : official publication of the European Spine Society, the  European Spinal Deformity Society, and the European Section of the Cervical Spine Research Society},
keywords = {Adolescent,Adult,Aged,Aged, 80 and over,Bone Screws,Female,Humans,Internal Fixators,Kyphoplasty,Lumbar Vertebrae,Male,Middle Aged,Orthopedic Procedures,Polymethyl Methacrylate,Radiography,Retrospective Studies,Spinal Fractures,Thoracic Vertebrae,Treatment Outcome,Young Adult,diagnostic imaging,injuries,instrumentation,methods,surgery},
