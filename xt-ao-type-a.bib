journal = {Neurology},
keywords = {Adult,Anticonvulsants,Basal Ganglia,Brain Injuries,Electroencephalography,Female,Humans,Magnetic Resonance Imaging,Male,Retrospective Studies,Seizures,Status Epilepticus,Young Adult,drug therapy,methods,pathology,therapeutic use},
language = {eng},
