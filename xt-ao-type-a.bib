language = {ger},
month = {aug},
number = {8},
pages = {703--709},
pmid = {23732614},
title = {{[Incomplete cranial burst fracture in the thoracolumbar junction. Results 6 years after thoracoscopic monosegmental spondylodesis].}},
volume = {117},
