journal = {European journal of radiology},
keywords = {Adult,Aged,Aged, 80 and over,Feasibility Studies,Female,Fractures, Malunited,Humans,Lumbar Vertebrae,Male,Middle Aged,Spinal Fractures,Surgery, Computer-Assisted,Thoracic Vertebrae,Tomography, X-Ray Computed,Treatment Outcome,Vertebroplasty,diagnostic imaging,injuries,methods,therapy},
language = {eng},
