}

@Article{Falco2014,
  author   = {de Falco, R and Bocchetti, A},
  title    = {{Balloon kyphoplasty for pure traumatic thoracolumbar fractures: retrospective analysis of 61 cases focusing on restoration of vertebral height.}},
