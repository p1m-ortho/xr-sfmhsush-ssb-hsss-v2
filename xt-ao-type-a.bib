}
@article{Ozturk2012,
abstract = {OBJECTIVE: This study aimed to research the effectiveness of customized thoracolumbosacral orthosis treatment for stable burst type thoracolumbar vertebral fractures without neurological deficits. METHODS: The study included 26 patients (14 males, 12 females; mean age: 46.03 years; range: 18 to 64 years) conservatively treated for thoracolumbar (T11-L2) burst type vertebral fractures according to Denis classification between 2002 and 2009. Etiology were a fall from various heights in 12 patients (46.2{\%}), motor vehicle accidents as an occupant in 7 (26.9{\%}) and as a pedestrian in 4 (15.4{\%}), and simple fall in 3 (11.5{\%}). None of the patients had neurologic deficit and no damage was found in the posterior ligamentous complex in MRI evaluations. Denis pain and functional scales were used in the clinical evaluation. Local kyphosis angle, sagittal index and height loss percentage were measured in the radiologic evaluation. Post-fracture and follow-up values were compared. Mean follow-up period was 41.30 (range: 14 to 80) months. RESULTS: Mean pain and functional scores were 1.65 and 1.15 points, respectively, at the final follow-up. Twenty patients returned to their pre-trauma work and activities completely and six patients with small limitations. Mean period for returning to work was 3.64 (range: 2 to 6) months. Local kyphosis angle, sagittal index and height loss percentage values increased significantly at follow-up (p{\textless}0.05). CONCLUSION: The conservative treatment of stable thoracolumbar burst fractures is widely accepted. Early mobilization with customized TLSO brace appears to produce effective functional results despite loss of vertebral body height.},
author = {Ozturk, Irfan and Erturer, Erden and Sonmez, Mehmet Mesut and Sari, Seckin and Seker, Ali and Seckin, Mustafa Faik},
issn = {2589-1294 (Electronic)},
journal = {Acta orthopaedica et traumatologica turcica},
keywords = {Adolescent,Adult,Braces,Cohort Studies,Early Ambulation,Female,Follow-Up Studies,Fracture Healing,Fractures, Compression,Humans,Injury Severity Score,Lumbar Vertebrae,Male,Middle Aged,Precision Medicine,Prosthesis Design,Radiography,Range of Motion, Articular,Recovery of Function,Retrospective Studies,Spinal Fractures,Thoracic Vertebrae,Time Factors,Young Adult,classification,diagnostic imaging,injuries,methods,physiology,therapy},
