journal = {Journal of spinal disorders {\&} techniques},
keywords = {Aged,Aged, 80 and over,Bone Cements,Catheterization,Female,Follow-Up Studies,Fracture Fixation, Internal,Humans,Male,Middle Aged,Polymethyl Methacrylate,Prospective Studies,Pseudarthrosis,Spinal Fractures,Thoracic Vertebrae,Treatment Outcome,administration {\&} dosage,injuries,methods,surgery},
