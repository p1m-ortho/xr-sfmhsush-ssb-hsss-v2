journal = {Acta orthopaedica Scandinavica},
keywords = {Adult,Analysis of Variance,Cadaver,Compressive Strength,External Fixators,Humans,Lumbar Vertebrae,Materials Testing,Middle Aged,Pain,Radiography,Range of Motion, Articular,Sacrum,Spinal Fractures,Time Factors,Walking,diagnostic imaging,etiology,injuries,physiology,physiopathology,standards,surgery},
