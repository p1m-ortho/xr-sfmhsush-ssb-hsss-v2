journal = {Orthopaedics {\&} traumatology, surgery {\&} research : OTSR},
keywords = {Adult,Aged,Female,Fracture Fixation, Internal,Humans,Lumbar Vertebrae,Male,Middle Aged,Pedicle Screws,Radiography,Retrospective Studies,Spinal Fractures,Thoracic Vertebrae,Visual Analog Scale,Young Adult,diagnostic imaging,injuries,instrumentation,surgery},
language = {eng},
