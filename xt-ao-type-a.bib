journal = {Spine},
keywords = {Biomechanical Phenomena,Cervical Vertebrae,Humans,Ligamentum Flavum,Spinal Canal,Spinal Cord Compression,Spinal Fractures,Tomography, X-Ray Computed,complications,diagnostic imaging,etiology,injuries,pathology,physiopathology},
language = {eng},
