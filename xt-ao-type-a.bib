keywords = {Cervical Vertebrae,Consensus,Humans,Magnetic Resonance Imaging,Neck Injuries,Reproducibility of Results,Spinal Cord Injuries,Spinal Fractures,Spinal Injuries,classification,diagnostic imaging,injuries},
language = {eng},
