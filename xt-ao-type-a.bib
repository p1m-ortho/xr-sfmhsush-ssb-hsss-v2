language = {eng},
number = {4},
pages = {403--411},
pmid = {1762000},
title = {{Spinal canal decompression in traumatic thoracolumbar burst fractures: posterior  distraction rods versus transpedicular screw fixation.}},
volume = {5},
year = {1991}
}
