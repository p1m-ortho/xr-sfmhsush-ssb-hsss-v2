keywords = {Accidents, Traffic,Adolescent,Adult,Arthrodesis,Bone Transplantation,Decompression, Surgical,Female,Fracture Fixation, Internal,Humans,Laminectomy,Lumbar Vertebrae,Male,Middle Aged,Operative Time,Pain Measurement,Pedicle Screws,Postoperative Period,Reconstructive Surgical Procedures,Spinal Fractures,Thoracic Vertebrae,Young Adult,surgery},
language = {chi},
month = {aug},
number = {8},
