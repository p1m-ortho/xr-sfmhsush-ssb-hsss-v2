journal = {Zhongguo gu shang = China journal of orthopaedics and traumatology},
keywords = {Adult,Aged,Aged, 80 and over,Bone Transplantation,Female,Fracture Fixation, Internal,Humans,Lumbar Vertebrae,Male,Middle Aged,Minimally Invasive Surgical Procedures,Spinal Fractures,Thoracic Vertebrae,injuries,methods,surgery},
