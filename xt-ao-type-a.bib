file = {:Users/anonymous/Library/Application Support/Mendeley Desktop/Downloaded/Siebenga et al. - 2006 - Treatment of Traumatic Thoracolumbar Spine Fractures A Multicenter Prospective Randomized Study of Operative Ve.pdf:pdf},
isbn = {0362-2436},
issn = {0362-2436},
journal = {Spine},
number = {25},
pages = {2881--2890},
pmid = {17139218},
title = {{Treatment of Traumatic Thoracolumbar Spine Fractures: A Multicenter Prospective Randomized Study of Operative Versus Nonsurgical Treatment}},
url = {https://insights.ovid.com/crossref?an=00007632-200612010-00003},
volume = {31},
