keywords = {Adult,Aged,Aged, 80 and over,Bone Cements,Bone Screws,Female,Follow-Up Studies,Humans,Kyphoplasty,Lumbar Vertebrae,Male,Middle Aged,Osteoporotic Fractures,Patient Positioning,Polymethyl Methacrylate,Postoperative Complications,Radiography,Spinal Fractures,Spinal Fusion,Thoracic Vertebrae,diagnostic imaging,etiology,instrumentation,methods,surgery},
language = {ger},
