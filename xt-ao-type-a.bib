keywords = {Adult,Bone Screws,Female,Follow-Up Studies,Fracture Fixation, Internal,Humans,Lumbar Vertebrae,Male,Middle Aged,Prosthesis Failure,Reoperation,Retrospective Studies,Spinal Fractures,Thoracic Vertebrae,Time Factors,Young Adult,diagnosis,etiology,injuries,methods,surgery},
language = {eng},
month = {may},
number = {2},
