issn = {0529-5815 (Print)},
journal = {Zhonghua wai ke za zhi [Chinese journal of surgery]},
keywords = {Adult,Aged,Animals,Cattle,Female,Fracture Fixation, Internal,Fractures, Bone,Humans,Lumbar Vertebrae,Male,Middle Aged,Thoracic Vertebrae,injuries,instrumentation,surgery},
