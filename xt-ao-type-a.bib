keywords = {Adolescent,Adult,Aged,Aged, 80 and over,Child,Child, Preschool,China,Cohort Studies,Disasters,Earthquakes,Female,Humans,Infant,Male,Middle Aged,Retrospective Studies,Spinal Injuries,Tomography, X-Ray Computed,Young Adult,diagnostic imaging,epidemiology,etiology,methods},
language = {eng},
