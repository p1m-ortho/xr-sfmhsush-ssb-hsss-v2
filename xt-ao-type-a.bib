journal = {Zhongguo gu shang = China journal of orthopaedics and traumatology},
keywords = {Aged,Bone Cements,Female,Fracture Fixation, Internal,Humans,Lumbar Vertebrae,Male,Middle Aged,Osteoporotic Fractures,Pedicle Screws,Spinal Fractures,Thoracic Vertebrae,Treatment Outcome,Vertebroplasty,etiology,injuries,methods,surgery,therapeutic use},
language = {chi},
