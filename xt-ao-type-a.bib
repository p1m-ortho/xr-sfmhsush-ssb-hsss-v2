}

@Article{Falco2005,
  author   = {de Falco, R and Scarano, E and {Di Celmo}, D and Grasso, U and Guarnieri, L},
  title    = {{Balloon kyphoplasty in traumatic fractures of the thoracolumbar junction. Preliminary experience in 12 cases.}},
  journal  = {Journal of neurosurgical sciences},
  year     = {2005},
  volume   = {49},
