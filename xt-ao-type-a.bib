keywords = {China,Humans,Lumbar Vertebrae,Observer Variation,Reproducibility of Results,Severity of Illness Index,Spinal Injuries,Surgeons,Thoracic Vertebrae,classification,diagnosis,injuries,statistics {\&} numerical data},
