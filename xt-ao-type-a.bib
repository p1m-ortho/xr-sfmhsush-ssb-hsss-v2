journal = {Asian Pacific journal of allergy and immunology},
keywords = {1-Acylglycerophosphocholine O-Acyltransferase,Animals,Asthma,Bronchoalveolar Lavage Fluid,Cytokines,Female,Genetic Vectors,HEK293 Cells,Humans,Lentivirus,Male,Mice,Mice, Inbred BALB C,Ovalbumin,Rats,Rats, Sprague-Dawley,analysis,chemistry,genetics,immunology,therapy},
language = {eng},
